{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "properties": {
        "used": {
            "type": "object",
            "title": "used",
            "description": "Has some tags generated by the datapack that you may read.",
            "properties": {
                "has_been_used": {
                    "type": "boolean",
                    "default": false,
                    "description": "When the item has been used it will turn to 'true'."
                },
                "times_used": {
                    "type": "integer",
                    "default": 0,
                    "description": "Everytime the item has been used it will add one to this tag."
                }
            }
        },
        "moreNBT": {
            "type": "object",
            "title": "moreNBT",
            "description": "This tag is required for all items that use More NBT Tags Datapack. Get the latest format in-game by doing \"/function more_nbt:get_current_nbt_format\"",
            "properties": {
                "nbt_format": {
                    "type": "integer",
                    "default": 4,
                    "description": "The format of More NBT Datapack Tags."
                }
            }
        },
        "on_use": {
            "type": "object",
            "title": "onUse",
            "description": "When the player clicks on the item it will run the contents",
            "properties": {
                "add_mob_effect":{
                    "type": "array",
                    "description": "A list of effects the target will get.",
                    "properties": {
                        "target": {
                            "enum": ["@a","@e","@s","@p","@r","this"],
                            "default": "this",
                            "description": "The target context to execute against."
                        },
                        "effect": {
                            "type": "string",
                            "default": "minecraft:luck",
                            "description": "The minecraft effect."
                        },
                        "showIcon": {
                            "type": "boolean",
                            "default": true,
                            "description": "Show particles/icon."
                        }
                    }
                },
                "remove_mob_effect":{
                    "type": "array",
                    "description": "A list of effects that it will remove from the target.",
                    "properties": {
                        "target": {
                            "enum": ["@a","@e","@s","@p","@r","this"],
                            "default": "this",
                            "description": "The target context to execute against."
                        },
                        "effect": {
                            "type": "string",
                            "default": "minecraft:luck",
                            "description": "The minecraft effect."
                        }
                    }
                },
                "die":{
                    "type": "object",
                    "description": "Will kill the target.",
                    "properties": {
                        "target": {
                            "enum": ["@a","@e","@s","@p","@r","this"],
                            "default": "this",
                            "description": "The target context to execute against."
                        }
                    }
                },
                "transform_item":{
                    "type": "object",
                    "description": "Replace the current holding item with the new defined item.",
                    "properties": {
                        "transform": {
                            "type":"string",
                            "default": "minecraft:paper",
                            "description": "The item that will be replaced with."
                        }
                    }
                },
                "replaceitem": {
                    "type": "object",
                    "description": "This will copy and move the item to the new selected slot.",
                    "properties": {
                        "slot": {
                            "type": "string",
                            "default": "weapon.mainhand",
                            "description": "This is the slot the item will end up in."
                        },
                        "replace": {
                            "type": "boolean",
                            "default": false,
                            "description": "Fail if their already is a item in that slot."
                        }
                    }
                },
                "merge_data": {
                    "type": "object",
                    "description": "Will apply the data."
                },
                "show_text": {
                    "type": "object",
                    "description": "Clears title, subtitle, and actionbar",
                    "properties": {
                        "clear": {
                            "type": "object",
                            "description": "Will clear text",
                            "properties": {
                                "target": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                }
                            }
                        },
                        "say": {
                            "type": "object",
                            "description": "Tells message from the user.",
                            "properties": {
                                "text": {
                                    "type": "string",
                                    "description": "The message to display"
                                }
                            }
                        },
                        "tellraw": {
                            "type": "object",
                            "description": "Tells the rawtext to chat.",
                            "properties": {
                                "target": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                },
                                "raw_text": {
                                    "type":"string",
                                    "description": "The JSON formatted text to show."
                                }
                            }
                        },
                        "title": {
                            "type": "object",
                            "description": "Tells the rawtext to title.",
                            "properties": {
                                "target": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                },
                                "raw_text": {
                                    "type":"string",
                                    "description": "The JSON formatted text to show."
                                }
                            }
                        },
                        "subtitle": {
                            "type": "object",
                            "description": "Tells the rawtext to subtitle.",
                            "properties": {
                                "target": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                },
                                "raw_text": {
                                    "type":"string",
                                    "description": "The JSON formatted text to show."
                                }
                            }
                        },
                        "actionbar": {
                            "type": "object",
                            "description": "Tells the rawtext to actionbar.",
                            "properties": {
                                "target": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                },
                                "raw_text": {
                                    "type":"string",
                                    "description": "The JSON formatted text to show."
                                }
                            }
                        }
                    }
                },
                "decrement_stack": {
                    "type": "object",
                    "description": "Will remove the selected item/weapon.main hand.",
                    "properties": {
                        "gamemode": {
                            "enum": ["all","survival","spectator","creative","adventure","!survival","!spectator","!creative","!adventure"],
                            "default": "all",
                            "description": "A list of gamemodes that will remove the item. Put \"all\" for all gamemodes."
                        }
                    }
                },
                "play_sound": {
                    "type": "object",
                    "description": "Will play the sound when the item is used.",
                    "properties": {
                        "target": {
                            "enum": ["@a","@e","@s","@p","@r","this"],
                            "default": "this",
                            "description": "The target context to execute against."
                        },
                        "sound": {
                            "type":"string",
                            "default": "minecraft:block.stone.dig",
                            "description": "The sound to play. Only some are currently supported."
                        }
                    }
                },
                "score": {
                    "type": "object",
                    "description": "add, remove, set scores.",
                    "properties": {
                        "add": {
                            "type": "object",
                            "description": "adds score.",
                            "properties": {
                                "name": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                },
                                "objective": {
                                    "enum": ["score.test0","score.test1","score.test2","score.test3","score.test4","score.test5","score.test6","score.test7","score.test8","score.test9"],
                                    "default": "score.test0",
                                    "description": "Objective to use."
                                },
                                "amount": {
                                    "type": "integer",
                                    "default": 0,
                                    "description": "How much to add to score."
                                }
                            }
                        },
                        "set": {
                            "type": "object",
                            "description": "sets score.",
                            "properties": {
                                "name": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                },
                                "objective": {
                                    "enum": ["score.test0","score.test1","score.test2","score.test3","score.test4","score.test5","score.test6","score.test7","score.test8","score.test9"],
                                    "default": "score.test0",
                                    "description": "Objective to use."
                                },
                                "amount": {
                                    "type": "integer",
                                    "default": 0,
                                    "description": "How much to set to score."
                                }
                            }
                        },
                        "remove": {
                            "type": "object",
                            "description": "adds score.",
                            "properties": {
                                "name": {
                                    "enum": ["@a","@e","@s","@p","@r","this"],
                                    "default": "this",
                                    "description": "The target context to execute against."
                                },
                                "objective": {
                                    "enum": ["score.test0","score.test1","score.test2","score.test3","score.test4","score.test5","score.test6","score.test7","score.test8","score.test9"],
                                    "default": "score.test0",
                                    "description": "Objective to use."
                                },
                                "amount": {
                                    "type": "integer",
                                    "default": 0,
                                    "description": "How much to remove from the score."
                                }
                            }
                        }
                    }
                },
                "damage": {
                    "type": "object",
                    "description": "Will damage the target.",
                    "properties": {
                        "target": {
                            "enum": ["@a","@e","@s","@p","@r","this"],
                            "default": "this",
                            "description": "The target context to execute against."
                        },
                        "type": {
                            "enum": ["EFFECT","ARROW"],
                            "description": "The way to apply the damage."
                        },
                        "amount": {
                            "type": "integer",
                            "default": 2,
                            "description": "How many half hearts to apply to target."
                        }
                    }
                },
                "reload": {
                    "type":"object",
                    "description": "Will reload the server. if it's a modded server (Spigot, Bukkit, Paper) it will run '/reload confirm'",
                    "properties": {
                        "confirm": {
                            "type": "boolean",
                            "default": false,
                            "description": "Must be set to true for it to reload the server."
                        }
                    }
                },
                "random": {
                    "type": "object",
                    "description": "Will output a random number from the min and max numbers. This can be called from player tag 'SelectedItem.tag.onUse.random.result'",
                    "properties": {
                        "min": {
                            "type": "integer",
                            "default": 0,
                            "description": "The minium number to use. Must be less than max"
                        },
                        "max": {
                            "type": "integer",
                            "default": 1,
                            "description": "The maxium number to use. Must be more than min"
                        },
                        "result": {
                            "type": "integer",
                            "description": "This is where the output will end up."
                        }
                    }
                
                },
                "summon": {
                    "type": "object",
                    "description": "Summon an entity with optional nbt data.",
                    "properties": {
                        "placement": {
                            "enum": ["ON_PLAYER","ON_BLOCK"],
                            "default": "ON_PLAYER",
                            "description": "Where to place the entity. This may get removed."
                        },
                        "entity": {
                            "type": "string",
                            "default": "minecraft:area_effect_cloud",
                            "description": "The entity to summon"
                        },
                        "nbt": {
                            "type": "object",
                            "description": "The nbt to apply to the entity."
                        }
                    }
                },
                "structure": {
                    "type": "object",
                    "description": "Places a structure.",
                    "properties": {
                        "structure_name": {
                            "type": "string",
                            "default": "more_nbt:test",
                            "description": "The structure to place."
                        },
                        "pos": {
                            "type": "array",
                            "default": [0,1,0],
                            "description": "The position / offset of the structure."
                        },
                        "ignoreEntities": {
                            "type": "boolean",
                            "default": false,
                            "description": "Place entities that are stored in the structure file."
                        },
                        "powered": {
                            "type": "boolean",
                            "default": true,
                            "description": "This will make the strcuture automatically load when the item is used."
                        }
                    }
                },
                "loot": {
                    "type": "object",
                    "description": "Get the loot from a loottable.",
                    "properties": {
                        "target": {
                            "enum": ["spawn","give"],
                            "description": "How to get the resulting items."
                        },
                        "source": {
                            "enum": ["loot","kill"],
                            "description": "Where to get the loot from."
                        },
                        "table": {
                            "type": "string",
                            "description": "The loot table to use when source=loot"
                        },
                        "entity": {
                            "type": "object",
                            "description": "The loot table to use when source=kill",
                            "properties": {
                                "sort": {
                                    "enum": ["nearest"],
                                    "description": "Where to look for the entity."
                                },
                                "type": {
                                    "type": "string",
                                    "description": "Entity to look for. Excepts all entities including \"player\""
                                }
                            }
                        }
                    }
                },
                "set_block": {
                    "type": "object",
                    "description": "Set a block with optional data",
                    "properties": {
                        "block_type": {
                            "type":"string",
                            "default":"minecraft:stone",
                            "description": "The block to place."
                        },
                        "pos": {
                            "type": "array",
                            "default": [0,0,0],
                            "description": "Offset on where to place the block."
                        },
                        "nbt": {
                            "type": "object",
                            "description": "Nbt data to apply to the block."
                        },
                        "blockstate": {
                            "type": "array",
                            "description": "Block state to apply to the block."
                        }
                    }
                },
                "run_command": {
                    "type": "object",
                    "description": "Will run the command or commands",
                    "properties": {
                        "command": {
                            "type": "string",
                            "default": "/give @p diamond 1",
                            "description": "The command to run."
                        }
                    }
                }
            }
        },
        "debugLog": {
            "type": "boolean",
            "default": false,
            "title": "debugLog",
            "description": "If true it will give an output log via game chat of what is happening and if their are any errors."
        },
        "damage_tool": {
            "type": "object",
            "title": "damge_tool",
            "description": "Will deal damage to the tool when used.",
            "properties": {
                "remove": {
                    "type": "integer",
                    "default": 1,
                    "description": "How much damage to remove from the tool"
                },
                "break_sound": {
                    "type": "boolean",
                    "default":true,
                    "description": "Use the break sound when the item has been conusmed/broken."
                },
                "disappers": {
                    "type": "boolean",
                    "default": true,
                    "description": "Make this item disapper/turn into air when broken."
                }
            }
        },
        "animation": {
            "type": "object",
            "title": "animation",
            "description": "Animated CustomModelData when held.",
            "properties": {
                "looping": {
                    "type": "boolean",
                    "default": false,
                    "description": "Make the animation run year round."
                },
                "run": {
                    "type": "integer",
                    "default": 1,
                    "description": "The amount of times to run the animation. only if looping=false"
                },
                "frametime": {
                    "type": "integer",
                    "default": 20,
                    "description": "The speed that it wil flip through the models."
                },
                "frames": {
                    "type": "array",
                    "default": [1,2,3,4,5],
                    "description": "The CustomModelData integer to apply at that frame."
                }
            }
        },
        "throwable": {
            "type": "object",
            "title": "throwable",
            "description": "Throwable item componet. Throwable items, such as snowball. Right now the entity will only go in the south direction, does not use player direction. This is planned to change.",
            "properties": {
                "ammunition": {
                    "type": "object",
                    "description": "Throwable item componet. Throwable items, such as snowball.",
                    "properties": {
                        "entity": {
                            "type": "string",
                            "default": "minecraft:cow",
                            "description": "The item/entity to throw"
                        },
                        "nbt": {
                            "type": "array",
                            "description": "Nbt That the entity has."
                        }
                    }
                }
            }
        },
        "shooter": {
            "type": "object",
            "title": "shooter",
            "description": "Similar to 'throwable' but acts more like a bow.",
            "properties": {
                "max_draw_duration": {
                    "type": "integer",
                    "default": 0,
                    "description": "Draw Duration."
                },
                "max_launch_power": {
                    "type": "integer",
                    "default": 0,
                    "description": "Launch power."
                },
                "ammunition": {
                    "type": "object",
                    "description": "Throwable item componet. Throwable items, such as snowball.",
                    "properties": {
                        "entity": {
                            "type": "string",
                            "default": "minecraft:cow",
                            "description": "The item/entity to throw"
                        },
                        "nbt": {
                            "type": "array",
                            "description": "Nbt That the entity has."
                        }
                    }
                }
            }
        },
        "Inventory": {
            "type": "object",
            "title": "Inventory",
            "description": "Inventory to store items.",
            "properties": {
                "can_be_siphoned_from": {
                    "type": "boolean",
                    "default": true,
                    "description": "Can hoppers suck items out of this inventory."
                },
                "inventory_size": {
                    "type": "integer",
                    "default": 26,
                    "description": "How many slot the inventory will have."
                },
                "inventory_layout": {
                    "enum": ["inline","center=top","center=bottom","center=middle"],
                    "description": "How to layout the slots."
                },
                "inventory_type": {
                    "enum": ["chest","hopper"],
                    "description": "The preset inventory to use. Keep into note that chests have 26 slots while hoppers have 5."
                },
                "auto_save": {
                    "type": "object",
                    "description": "Where to save the container items to.",
                    "properties": {
                        "storage": {
                            "type": "string",
                            "default": "item",
                            "description": "If set to item it will save the container to the item that the player is holding."
                        },
                        "save_fillers": {
                            "type": "boolean",
                            "default": false,
                            "description": "Stores the fake filler items that limit the storage space."
                        }
                    }
                },
                "filter": {
                    "type": "object",
                    "description": "white/blacklisted items that are allowed in the slots.",
                    "properties": {
                        "whitelist": {
                            "type": "array",
                            "description": "Items that are allowed."
                        },
                        "blacklist": {
                            "type":"array",
                            "description": "Items that are not allowed."
                        }
                    }
                }
            }
        },
        "new_ui": {
            "type": "object",
            "title": "new_ui",
            "description": "Custom UI to add buttons, toggles, viewers, etc. all below tags must be contained inside their own object. For regeranceing diffrent UI's.",
            "properties": {
                "button": {
                    "type": "object",
                    "description": "A item insde the inventory that will do stuff when you click it.",
                    "properties": {
                        "button_text": {
                            "type": "string",
                            "default":"Button",
                            "description": "The name for the button that shows up in-game"
                        },
                        "button_type": {
                            "enum": ["button","toggle"],
                            "default": "button",
                            "description": "The kind of button it is, button will turn on then off. toggle will turn on then when you click it again will turn off."
                        },
                        "button_pos": {
                            "type": "array",
                            "default": [0],
                            "description": "Where to place the button"
                        },
                        "button_size": {
                            "type": "array",
                            "default": [1],
                            "description": "The size of the button"
                        },
                        "button_style": {
                            "type": "object",
                            "default": "default",
                            "description": "You can make a custom style/button or just put \"default\"",
                            "properties": {
                                "item": {
                                    "type": "string",
                                    "default": "minecraft:stone_button",
                                    "description": "Minecraft item to use."
                                },
                                "nbt": {
                                    "type": "object",
                                    "description": "Nbt data to apply to the item."
                                }
                            }
                        },
                        "button_use": {
                            "type": "object",
                            "description": "Functions of the button. true will run when the button is pressed or when toggled. false is the opposite.",
                            "properties": {
                                "true": {
                                    "type": "object",
                                    "description": "When the button gets pressed.",
                                    "properties": {
                                        "open_ui": {
                                            "type": "object",
                                            "description": "Opens a ui located on this item.",
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "description": "This is the integer id of the ui that is located on the item."
                                                }
                                            }
                                        },
                                        "close_ui": {
                                            "type": "boolean",
                                            "default": true,
                                            "description": "Will close the current UI."
                                        }
                                    }
                                },
                                "false": {
                                    "type": "object",
                                    "description": "When the button is not pressed."
                                }
                            }
                        }
                    }
                },
                "filler": {
                    "type": "object",
                    "description": "Places a fake item that looks like the background if you have the resourcepack installed.",
                    "properties": {
                        "all_empty": {
                            "type": "object",
                            "description": "Will fill all empty slots. You can use a preset item. or define your own.",
                            "properties": {
                                "item": {
                                    "type": "string",
                                    "default": "minecraft:light_gray_stained_glass_pane",
                                    "description": "Item's id that will be replaced in all the empty slots."
                                },
                                "nbt": {
                                    "type": "object",
                                    "default":{"CustomModelData":"1","display":{"Name":""}},
                                    "description": "The nbt data to apply to this item"
                                },
                                "preset_ui_item": {
                                    "type": "object",
                                    "default": {},
                                    "description": "Will use the preset UI item thats built-in."
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}